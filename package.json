{
  "private": true,
  "name": "fancy-helloworld",
  "workspaces": [
    "packages/*"
  ],
  "version": "1.0.0",
  "description": "React Redux Node.js sample project to give insights on how to wire a universal js app",
  "scripts": {
    "lint": "lerna run lint",
    "test": "lerna run test",
    "clean": "lerna run clean",
    "build": "lerna run build",
    "test:e2e": "lerna run test:e2e",
    "start": "pm2 start ecosystem.config.js",
    "stop": "pm2 stop ecosystem.config.js",
    "all": "lerna bootstrap && yarn lint && yarn test && yarn build && yarn start && yarn test:e2e"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "hot-reloading",
    "boilerplate",
    "webpack",
    "babel",
    "react-transform",
    "lerna",
    "yarn-workspaces"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:gacosta89/fancy-helloworld.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-project-relative-import": "^2.0.1",
    "cross-env": "^5.1.3",
    "enzyme": "^2.7.1",
    "eslint": "^4.15.0",
    "eslint-plugin-jest": "^21.6.1",
    "eslint-plugin-react": "^4.2.2",
