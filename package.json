{
  "name": "fancy-helloworld",
  "version": "1.0.0",
  "description": "React Redux Node.js sample project to give insights on how to wire a universal js app",
  "scripts": {
    "lint": "eslint --cache .",
    "test": "cross-env NODE_PATH=source babel-node source/test/index.js",
    "clean": "rimraf build",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:client": "webpack --config webpack.config.prod.js --bail",
    "build:dev": "webpack --config webpack.config.dev.js --bail",
    "build:server": "webpack --config webpack.config.server.js --bail",
    "deploy": "babel-node scripts/docker.js --build fancy-helloworld",
    "start": "babel-node scripts/docker.js --start fancy-helloworld",
    "stop": "babel-node scripts/docker.js --stop fancy-helloworld",
    "test:e2e": "babel-node scripts/e2e.js",
    "all": "npm run lint && npm run test && npm run build && babel-node scripts/docker.js --stop --clean fancy-helloworld && babel-node scripts/docker.js --build --start fancy-helloworld && npm run test:e2e",
    "dev": "cross-env NODE_PATH=source babel-node devServer.js",
    "watch": "watch \"clear && npm run lint -s && npm run test -s\" source"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "hot-reloading",
    "boilerplate",
    "webpack",
    "babel",
    "react-transform"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:gacosta89/fancy-helloworld.git"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.7",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.5.0",
    "cross-env": "^1.0.7",
    "enzyme": "^2.7.1",
    "eslint": "^3.5.0",
    "eslint-plugin-react": "^4.2.2",
    "estraverse-fb": "^1.3.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "nightwatch": "^0.9.12",
    "npm-which": "^3.0.1",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-transform-catch-errors": "^1.0.2",
    "redbox-react": "^1.2.2",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "rimraf": "^2.5.2",
    "selenium-standalone": "^6.0.1",
    "supertest": "^1.2.0",
    "tap-xunit": "^1.7.0",
    "tape": "^4.5.1",
    "watch": "^0.17.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.17.0",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "babel-polyfill": "^6.7.2",
    "bunyan-request-logger": "^1.1.0",
    "config": "^1.20.1",
    "connect-cache-control": "^1.0.0",
    "express": "^4.13.4",
    "i18next": "^7.0.1",
    "normalize.css": "^5.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-i18next": "^2.2.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-saga": "^0.12.0",
    "styled-components": "^1.4.2"
  },
  "license": "MIT",
  "homepage": "https://github.com/gacosta89/fancy-helloworld"
}
